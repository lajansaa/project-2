<form method="GET" action="/reports/new">
  <button class="ui primary button" type="submit">New Report</button>
</form>

<div>
  {{#each category}}
  <div>
    <i class="fas fa-folder-open"></i>
    <a href="/categories/{{this.id}}">{{this.title}}</a>
    <div class="ui icon top left pointing dropdown button">
      <i class="wrench icon"></i>
      <div class="menu">
        <div class="item"><a href="/categories/{{this.id}}/edit">Rename Category</a></div>
        <div class="item" href="/categories/{{this.id}}/delete">Delete Category</div>
      </div>
    </div>
  </div>
  {{/each}}
</div>



